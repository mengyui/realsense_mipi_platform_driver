	i2c@3180000 {
		status = "okay";
		clock-frequency = <100000>;
		tca9548@72 {
			status = "okay";
			reg = <0x72>;
			compatible = "nxp,pca9548";
			#address-cells = <1>;
			#size-cells = <0>;
			skip_mux_detect = "yes";
			vcc-supply = <&p2822_vdd_1v8_cvb>;
